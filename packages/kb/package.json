{
  "name": "@haais/kb",
  "version": "1.1.0",
  "description": "HAAIS Knowledge Layer - Governed semantic search for Custom GPT Actions",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx server.ts",
    "ingest": "tsx ingest-md.ts",
    "ingest:md": "tsx ingest-md.ts",
    "import:public": "tsx cli/import-public-sources.ts --config configs/cleveland.config.json",
    "import:public:dry": "tsx cli/import-public-sources.ts --config configs/cleveland.config.json --dry-run",
    "import:socrata": "tsx cli/import-public-sources.ts --config configs/cleveland.config.json --type socrata",
    "import:legistar": "tsx cli/import-public-sources.ts --config configs/cleveland.config.json --type legistar",
    "import:web": "tsx cli/import-public-sources.ts --config configs/cleveland.config.json --type webPages",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "node --test --experimental-test-coverage __tests__/*.test.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
